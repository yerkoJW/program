// garbaje.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main()
{
	int i,n,p;
	i=1;
	p=1;
	do{
	cout<<"ingrese el valor de n: "<<endl;
	cin>>n;
	}while (n<0);
	while (i<=n)
	{
		p=p*i;
		i=i+1;
	}
	cout<<" el factorial es: "<<p<<endl;
	getch();
}
