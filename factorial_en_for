// garbaje.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main()
{
	int i,n,p;
	p=1;
	do{
	cout<<"ingrese el valor de n: "<<endl;
	cin>>n;
	}while (n<0);

	for (i=1;i<=n;i++)	
		p=p*i;
	cout<<"el producto es: "<<p<<endl;
	getch();
}
